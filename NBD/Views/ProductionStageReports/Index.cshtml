            <th>
                @Html.DisplayNameFor(model => model.EstLaborProdCost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActuLaborDesingCost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstLaborDesingCost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Project)
            </th>
            <th></th>
